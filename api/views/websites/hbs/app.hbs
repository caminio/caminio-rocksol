        <li>{{#link-to 'webpages' activeClass='active'}}{{t 'nav.webpages'}}{{/link-to}}</li>
        {{#if blogEnabled}}
          <li>{{#link-to 'blog' activeClass='active'}}{{t 'nav.blog'}}{{/link-to}}</li>
